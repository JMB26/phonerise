{% extends 'base.html.twig' %}

{% block title %}Detailcommande index{% endblock %}

{% block body %}
    <h1>Detailcommande index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Titre</th>
                <th>Quantity</th>
                <th>Prix_unit</th>
                <th>Total</th>
                <th>Ref</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for detailcommande in detailcommandes %}
            <tr>
                <td>{{ detailcommande.id }}</td>
                <td>{{ detailcommande.titre }}</td>
                <td>{{ detailcommande.quantity }}</td>
                <td>{{ detailcommande.prixUnit }}</td>
                <td>{{ detailcommande.total }}</td>
                <td>{{ detailcommande.ref }}</td>
                <td>
                    <a href="{{ path('app_detailcommande_show', {'id': detailcommande.id}) }}">show</a>
                    <a href="{{ path('app_detailcommande_edit', {'id': detailcommande.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_detailcommande_new') }}">Create new</a>
{% endblock %}
